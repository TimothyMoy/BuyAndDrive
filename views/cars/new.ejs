  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="../styles/profile.css">
  <link href="https://fonts.googleapis.com/css2?family=Oleo+Script:wght@700&display=swap" rel="stylesheet">
  <title>Add Car</title>
</head>
<body>
  <%- include('../partials/_nav') %>
  <main>
    <h1>Add Car</h1>
  
    <form action="/cars" method="POST">
      <label for="make">Make</label>
      <input type="text" id="name" name="make"/>
      <label for="model">Model</label>
      <input type="text" id="name" name="model"/>
      <label for="milesDriven">Miles driven</label>
      <input type="number" id="milesDriven" name="milesDriven"/>
      <label for="mpg">MPG</label>
      <input type="number" id="mpg" name="mpg"/>
      <label for="proposedPrice">Proposed Price</label>
      <input type="number" id="proposedPrice" name="proposedPrice"/>
      <label for="img">Img Link:</label>
      <input type="text" id="img" name="img"/>
      <button type="submit" value="upload">Add Car</button>
    </form>
    
 
    
</body>
